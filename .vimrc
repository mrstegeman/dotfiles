set nocompatible

filetype plugin indent on
set autoindent
set backspace=indent,eol,start
set completeopt=longest,menuone
set expandtab
set ff=unix
set formatoptions=l
set incsearch
set lbr
set mouse=a
set ruler
set scrolloff=5
set shell=/bin/bash
set shiftwidth=4
set showmatch
set showmode
set smartindent
set softtabstop=4
set tabstop=4
set title
syntax on
