# touchpad
"/home/michael/bin/touchtoggle.sh"
    m:0x0 + c:199
    XF86TouchpadToggle

# brightness down
"xbacklight -dec 10"
    m:0x0 + c:232
    XF86MonBrightnessDown

# brightness up
"xbacklight -inc 10"
    m:0x0 + c:233
    XF86MonBrightnessUp

# prev
"ncmpcpp prev"
    m:0x0 + c:173
    XF86AudioPrev

# play/pause
"ncmpcpp toggle"
    m:0x0 + c:172
    XF86AudioPlay

# next
"ncmpcpp next"
    m:0x0 + c:171
    XF86AudioNext

